<link rel="stylesheet" type="text/css" href="<@spring.url '/ezSafe/qyda/qyxx/qyxq/qyxq.css'/>"/>

<div class="qyjbxx">
    <div class="qyjbxx-title">
        <span class="left-blue-icon"></span>
        <span>企业基本信息</span>
    </div>
    <div class="line-blue-1"></div>
    <div class="qyjbxx-content">
        <div class="qyjbxx-tab">
            <div class="short">企业名称</div>
            <div class="long" data-bind="text:dataKendo.QYMC"></div>
            <div class="short">机构代码</div>
            <div class="long" data-bind="text:dataKendo.ZZJGDM"></div>
            <div class="short">信用编码</div>
            <div class="long" data-bind="text:dataKendo.TYSHXYBM"></div>

            <div class="short">注册类型</div>
            <div class="long" data-bind="text:dataKendo.ZCLX.VALUE"></div>
            <div class="short">经营地址</div>
            <div class="long" data-bind="text:dataKendo.JYDZ"></div>
            <div class="short">工商注册号</div>
            <div class="long" data-bind="text:dataKendo.GSZCH"></div>

            <div class="short">企业状态</div>
            <div class="long" data-bind="text:dataKendo.QYZT.VALUE"></div>
            <div class="short">隶属行政</div>
            <div class="long" data-bind="text:dataKendo.LSXZ.VALUE"></div>
            <div class="short">所属街镇</div>
            <div class="long" data-bind="text:dataKendo.SSJZ.JZMC"></div>

            <div class="short">地理位置</div>
            <div class="long" data-bind="text:dataKendo.DLWZ.VALUE"></div>
            <div class="short">所属辖区</div>
            <div class="long" data-bind="text:dataKendo.SSXQ.XZQHMC"></div>
            <div class="short">邮政编码</div>
            <div class="long" data-bind="text:dataKendo.YZBM"></div>
        </div>
    </div>
</div>

<div class="qyzcxx">
    <div class="qyjbxx-title">
        <span class="left-blue-icon"></span>
        <span>企业注册信息</span>
    </div>
    <div class="line-blue-2"></div>
    <div class="qyzcxx-content">
        <div class="qyzcxx-tab">
            <div class="short">企业性质</div>
            <div class="long" data-bind="text:dataKendo.QYXZ.VALUE"></div>
            <div class="short">企业分类</div>
            <div class="long four" data-bind="text:dataKendo.QYFL.VALUE"></div>
            <div class="short">注册资本</div>
            <div class="long" data-bind="text:dataKendo.ZCZB.JE"></div>

            <div class="short">成立日期</div>
            <div class="long" data-bind="text:dataKendo.CLRQ"></div>
            <div class="short">投产日期</div>
            <div class="long four" data-bind="text:dataKendo.TCRQ"></div>
            <div class="short">投资方国籍</div>
            <div class="long" data-bind="text:dataKendo.TZFGJ.VALUE"></div>

            <div class="short">所属行业领域</div>
            <div class="long" data-bind="text:dataKendo.SSHYLY.VALUE"></div>
            <div class="short">国民经济行业</div>
            <div class="long four" data-bind="text:dataKendo.GMJJHY.VALUE"></div>
            <div class="short">投资总额</div>
            <div class="long" data-bind="text:dataKendo.TZZE.JE"></div>

            <div class="short">所属行业集团</div>
            <div class="long" data-bind="text:dataKendo.SSHYJT"></div>
            <div class="short">上级公司名称</div>
            <div class="long four" colspan="3" data-bind="text:dataKendo.SJGSMC"></div>
            <div class=""></div>
            <div class=""></div>

            <div class="short">经营范围</div>
            <div class="long" colspan="5" data-bind="text:dataKendo.JYFW"></div>
            <div class=""></div>
            <div class=""></div>
            <div class=""></div>
            <div class=""></div>
        </div>
    </div>
</div>

<div class="qygmxx">
    <div class="qyjbxx-title">
        <span class="left-blue-icon"></span>
        <span>企业规模信息</span>
    </div>
    <div class="line-blue-3"></div>
    <div class="qygmxx-content">
        <div class="qygmxx-tab">
            <div class="short">企业规模</div>
            <div class="long" data-bind="text:dataKendo.QYGM.VALUE"></div>
            <div class="short">企业规模情况</div>
            <div class="long four" data-bind="text:dataKendo.QYGMQK"></div>
            <div class="short three">占地面积</div>
            <div class="long five" data-bind="text:dataKendo.ZDMJ"></div>

            <div class="short">固定资产总额</div>
            <div class="long" data-bind="text:dataKendo.GDZCZE.JE"></div>
            <div class="short">年销售收入总额</div>
            <div class="long four" data-bind="text:dataKendo.NXSSRZE.JE"></div>
            <div class="short three">年利润总额</div>
            <div class="long five" data-bind="text:dataKendo.NLRZE.JE"></div>

            <div class="short">仓库建筑面积</div>
            <div class="long two" data-bind="text:dataKendo.CKJZMJ"></div>
            <div class="short">办公楼建筑面积</div>
            <div class="long four" data-bind="text:dataKendo.BGLJZMJ"></div>
            <div class="short three">车间厂房建筑面积</div>
            <div class="long five" data-bind="text:dataKendo.CJCFJZMJ"></div>

            <div class="short">从业人员数量</div>
            <div class="long" data-bind="text:dataKendo.CYRYSL"></div>
            <div class="short">安全工程师人数</div>
            <div class="long four" data-bind="text:dataKendo.AQGCSRS"></div>
            <div class="short three">专职应急管理人数</div>
            <div class="long five" data-bind="text:dataKendo.ZZYJGLRS"></div>
        </div>
    </div>
</div>